<% @lists.each do |list| %>
  <div class="card" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-title"><%= list.name %></h5>
      <%= link_to "view list", list_path(list)%>
    </div>
  </div>
<% end %>

<%= link_to "add new list", new_list_path, class: "btn btn-primary" %>
